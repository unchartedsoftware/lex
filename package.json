{
  "name": "@uncharted/lex",
  "description": "A preact framework for building token-based search bars",
  "version": "0.23.0",
  "publishConfig": {
    "registry": "nexus.uncharted.software"
  },
  "author": {
    "name": "Sean McIntyre",
    "email": "smcintyre@uncharted.software"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unchartedsoftware/lex.git"
  },
  "scripts": {
    "start": "npm run serve",
    "demo": "npm run serve-demos",
    "clean": "rimraf dist/",
    "build": "npm-run-all clean lint webpack:*",
    "webpack:js": "MINIFY=true webpack --config config/webpack.config.build.js",
    "webpack:jsmin": "webpack --config config/webpack.config.build.js",
    "webpack:scss": "mkdir -p dist/ && cp -R src/style/* dist/",
    "serve": "DEV_ONLY=true webpack-dev-server --config config/webpack.config.watch.js --open",
    "serve-demos": "DEV_ONLY=false webpack-dev-server --config config/webpack.config.watch.js --open",
    "lint": "eslint ./src",
    "prepublishOnly": "npm run build",
    "docs": "documentation build --config documentation.yml src/lex.jsx -f html -o docs",
    "serve-docs": "documentation serve --config documentation.yml --watch src/lex.jsx"
  },
  "main": "dist/lex.js",
  "style": "dist/lex.css",
  "module": "src/lex.jsx",
  "devDependencies": {
    "bootstrap-sass": "3.3.7",
    "babel-eslint": "8.0.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-preset-env": "1.6.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-preact-require": "1.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "documentation": "5.3.3",
    "element-resize-detector": "1.1.15",
    "eslint": "4.10.0",
    "eslint-config-semistandard-react": "4.2.0",
    "eslint-plugin-semistandard-react": "4.1.0",
    "eslint-config-semistandard": "11.0.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-standard": "3.0.1",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-jsdoc": "3.1.3",
    "if-env": "1.0.0",
    "preact": "8.4.2",
    "moment": "2.24.0",
    "moment-timezone": "0.5.23",
    "flatpickr": "4.5.2",
    "wolfy87-eventemitter": "5.2.4",
    "preact-portal": "1.1.x",
    "rimraf": "2.6.2",
    "npm-run-all": "4.1.5",
    "webpack": "3.10.0",
    "clean-webpack-plugin": "0.1.17",
    "extract-text-webpack-plugin": "3.0.2",
    "uglifyjs-webpack-plugin": "1.1.2",
    "webpack-dev-server": "2.9.7",
    "sass-loader": "6.0.6",
    "css-loader": "0.28.7",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "file-loader": "1.1.5",
    "node-sass": "4.7.2",
    "babel-loader": "7.1.2",
    "eslint-loader": "2.0.0"
  },
  "peerDependencies": {
    "element-resize-detector": "1.1.x",
    "preact": "8.x",
    "moment": "2.22.x",
    "moment-timezone": "0.5.x",
    "flatpickr": "4.5.x"
  },
  "dependencies": {
    "lodash": "4.17.10",
    "lodash-decorators": "5.0.0"
  }
}
