{
  "name": "@uncharted/lex",
  "description": "A preact framework for building token-based search bars",
  "version": "0.23.11",
  "publishConfig": {
    "registry": "nexus.uncharted.software"
  },
  "author": {
    "name": "Sean McIntyre",
    "email": "smcintyre@uncharted.software"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unchartedsoftware/lex.git"
  },
  "scripts": {
    "start": "npm run serve",
    "demo": "npm run serve-demos",
    "clean": "rimraf dist/",
    "build": "npm-run-all clean lint webpack:*",
    "webpack:js": "cross-env MINIFY=true webpack --config config/webpack.config.build.js",
    "webpack:jsmin": "webpack --config config/webpack.config.build.js",
    "webpack:scss": "mkdirp dist/ && cp -R src/style/* dist/",
    "serve": "cross-env DEV_ONLY=true webpack-dev-server --config config/webpack.config.watch.js --open",
    "serve-demos": "cross-env DEV_ONLY=false webpack-dev-server --config config/webpack.config.watch.js --open",
    "lint": "eslint ./src",
    "prepublishOnly": "npm run build",
    "docs": "documentation build --config documentation.yml src/lex.jsx -f html -o docs",
    "serve-docs": "documentation serve --config documentation.yml --watch src/lex.jsx"
  },
  "main": "dist/lex.js",
  "style": "dist/lex.css",
  "module": "src/lex.jsx",
  "devDependencies": {
    "preact": "8.5.2",
    "moment": "2.24.0",
    "moment-timezone": "0.5.27",
    "flatpickr": "4.6.3",
    "wolfy87-eventemitter": "5.2.8",
    "preact-portal": "1.1.3",
    "bootstrap-sass": "3.4.1",
    "babel-eslint": "10.0.3",
    "@babel/runtime": "7.7.2",
    "@babel/preset-env": "7.7.1",
    "@babel/plugin-proposal-decorators": "7.7.0",
    "babel-plugin-preact-require": "1.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "@babel/plugin-transform-react-jsx": "7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/plugin-transform-runtime": "7.6.2",
    "documentation": "12.1.3",
    "eslint": "6.6.0",
    "eslint-config-semistandard-react": "4.2.0",
    "eslint-plugin-semistandard-react": "4.1.0",
    "eslint-config-semistandard": "15.0.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-jsdoc": "17.1.0",
    "cross-env": "6.0.3",
    "if-env": "1.0.4",
    "mkdirp": "0.5.1",
    "element-resize-detector": "1.1.15",
    "rimraf": "3.0.0",
    "npm-run-all": "4.1.5",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "clean-webpack-plugin": "3.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "uglifyjs-webpack-plugin": "2.2.0",
    "webpack-dev-server": "3.9.0",
    "sass-loader": "8.0.0",
    "css-loader": "3.2.0",
    "style-loader": "1.0.0",
    "url-loader": "2.2.0",
    "file-loader": "4.2.0",
    "node-sass": "4.13.0",
    "babel-loader": "8.0.6",
    "eslint-loader": "3.0.2"
  },
  "peerDependencies": {
    "element-resize-detector": "1.1.x",
    "preact": "8.x",
    "moment": "2.24.x",
    "moment-timezone": "0.5.x",
    "flatpickr": "4.5.x"
  },
  "dependencies": {
    "lodash": "4.17.15",
    "lodash-decorators": "5.0.0"
  }
}
